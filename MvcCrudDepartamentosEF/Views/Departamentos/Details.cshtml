@model Departamento

<div class="container mt-5">
    <h1>Detalles Departamento</h1>

    <p>
        <a asp-controller="Departamentos" asp-action="Index" class="btn btn-info">
            Atras
        </a>
    </p>

    <ul class="list-group">
        <li class="list-group-item">
            Id: @Model.idDepartamento
        </li>
        <li class="list-group-item">
            Nombre: @Model.Nombre
        </li>
        <li class="list-group-item">
            Localidad: @Model.Localidad
        </li>
    </ul>
</div>